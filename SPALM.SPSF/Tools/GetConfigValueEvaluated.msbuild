<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="3.5" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<!-- This file imports the config file (e.g. Application.config) and returns a given configuration value -->
	
	<Import Project="$(ConfigFileName)" />
	
	<Target Name="GetValue">
		<Message Text="STARTSECTION" Importance="High" />
		<Message Text="$(DebuggingWebApp)" Importance="High" />
		<Message Text="ENDSECTION" Importance="High" />
	</Target>	

</Project>