    <Icon>spsf.ico</Icon>
    <PreviewImage>spsf.jpg</PreviewImage>
    <Tags>SharePoint, SP2010, SP2013, VS2012, GAX, Guidance Automation Extensions, Factory</Tags>
  </Metadata>
  <Installation AllUsers="true">
    <InstallationTarget Version="11.0" Id="Microsoft.VisualStudio.Pro" />
    <InstallationTarget Version="11.0" Id="Microsoft.VisualStudio.Ultimate" />
    <InstallationTarget Version="11.0" Id="Microsoft.VisualStudio.Premium" />
  </Installation>
  <Dependencies>
    <Dependency Id="Microsoft.Framework.NDP" DisplayName="Microsoft .NET Framework" d:Source="Manual" Version="4.5" d:InstallSource="Download" />
